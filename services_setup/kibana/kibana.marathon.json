{
  "id": "kibana",
  "cmd": "/usr/local/sbin/inContainerStartService.sh",
  "cpus": 0.3,
  "mem": 700,
  "disk": 250,
  "instances": 1,
  "container": {
    "docker": {
      "image": "marathon.registry:5000/kibana",
      "forcePullImage": true,
      "parameters": [
        { "key": "user", "value": "elasticsearch" }
      ]
    },
    "type": "DOCKER",
    "network": "BRIDGE",
    "portMappings": [
      {
        "containerPort": 5601,
        "hostPort": 31561,
        "protocol": "tcp",
        "name": "httpkibana",
        "labels": null
      }
    ],
    "volumes": [
      {
        "containerPath": "/var/log/kibana",
        "hostPath": "/var/log/kibana",
        "mode": "RW"
      },
      {
        "hostPath": "/var/run/kibana",
        "containerPath": "/var/run/kibana",
        "mode": "RW"
      },
      {
        "hostPath": "/etc/eskimo_topology.sh",
        "containerPath": "/etc/eskimo_topology.sh",
        "mode": "RO"
      },
      {
        "hostPath": "/etc/eskimo_services-config.json",
        "containerPath": "/etc/eskimo_services-config.json",
        "mode": "RO"
      }
    ]
  },
  "networks": [
    {
      "mode": "container/bridge"
    }
  ],
  "env": {},
  "labels": {},
  "healthChecks": [
    {
      "protocol": "MESOS_TCP",
      "portIndex": 0,
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 3
    }
  ],
  "user": "elasticsearch"
}
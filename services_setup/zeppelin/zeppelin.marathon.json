{
  "id": "zeppelin",
  "cmd": "/usr/local/sbin/inContainerStartService.sh",
  "cpus": 0.6,
  "mem": 800,
  "disk": 800,
  "instances": 1,
  "container": {
    "docker": {
      "image": "marathon.registry:5000/zeppelin",
      "forcePullImage": true,
      "privileged": true
    },
    "type": "DOCKER",
    "network": "BRIDGE",
    "portMappings": [
      {
        "containerPort": 38080,
        "hostPort": 31008,
        "protocol": "tcp",
        "name": "httpzeppelin1",
        "labels": null
      },
      {
        "containerPort": 38081,
        "hostPort": 31009,
        "protocol": "tcp",
        "name": "httpzeppelin2",
        "labels": null
      }
    ],
    "volumes": [
      {
        "containerPath": "/var/log/zeppelin/",
        "hostPath": "/var/log/zeppelin",
        "mode": "RW"
      },
      {
        "hostPath": "/etc/eskimo_topology.sh",
        "containerPath": "/etc/eskimo_topology.sh",
        "mode": "RO"
      },
      {
        "hostPath": "/etc/eskimo_services-config.json",
        "containerPath": "/etc/eskimo_services-config.json",
        "mode": "RO"
      }
    ]
  },
  "networks": [
    {
      "mode": "container/bridge"
    }
  ],
  "env": {},
  "labels": {},
  "healthChecks": [
    {
      "protocol": "MESOS_TCP",
      "portIndex": 0,
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 3
    }
  ],
  "user": "spark"
}
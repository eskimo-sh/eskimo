  "kafka" : {
    "config": {
      "imageName" : "kafka",
      "order": 9,
      "group" : "Kafka",
      "name" : "Broker",
      "selectionLayout" : { "row" : 6, "col" : 3},
      "memory": "medium",
      "logo" : "images/kafka-logo.png",
      "icon" : "images/kafka-icon.png"
    },

    echo "cerebro.default elasticsearch.default flink-runtime.default flink-runtime-rest.default grafana.default kafka.default kafka-manager.default kibana.default logstash.default kube-dns.kube-system dashboard-metrics-scraper.kubernetes-dashboard kubernetes-dashboard.kubernetes-dashboard . "
fi


